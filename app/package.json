{
  "name": "webxraydb-rs",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "lint:ox": "oxlint --deny-warnings src",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "preview": "vite preview",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@tanstack/react-router": "^1",
    "@tanstack/react-start": "^1",
    "plotly.js-basic-dist-min": "^3",
    "react": "^19",
    "react-dom": "^19",
    "react-plotly.js": "^2"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.3.1",
    "@types/plotly.js": "^3.0.6",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-plotly.js": "^2.6.3",
    "@vitejs/plugin-react": "^4",
    "jsdom": "^27.0.0",
    "nitro": "3.0.1-alpha.2",
    "oxlint": "^1.26.0",
    "simple-git-hooks": "^2.13.1",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vite": "^7",
    "vite-tsconfig-paths": "^5",
    "vitest": "^3.2.4"
  },
  "simple-git-hooks": {
    "pre-commit": "cd \"$(git rev-parse --show-toplevel)/app\" && bun run lint:ox && bun run typecheck"
  }
}
